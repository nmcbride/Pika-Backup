msgid "Pika Backup"
msgstr "Kopie zapasowe Pika"

#: data/app.desktop.in:7
msgid "Backups"
msgstr "Kopie zapasowe"

#: data/app.desktop.in:8 data/app.metainfo.xml.in:5
msgid "Keep your data safe"
msgstr "Zabezpiecz swoje dane"

#: data/app.desktop.in:14
msgid "archive;back-up;borg;"
msgstr ""
"archiwum;archiwizowanie;archiwizuj;archive;kopia zapasowa;bekap;backup;back-"
