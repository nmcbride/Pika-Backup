msgid "URL"
msgstr "URL"

#: data/ui/dialog_add_config.ui:421
msgid "ssh://user@server.example.org/~/backup"
msgstr "ssh://user@server.example.org/~/backup"

#: data/ui/dialog_add_config.ui:471
