<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libhandy" version="1.0"/>
  <object class="GtkImage" id="detail_repo_icon">
    <style>
      <class name="row-icon"/>
    </style>
  </object>
  <menu id="app_menu">
    <section>
      <item>
        <attribute name="label" translatable="yes">_About Pika Backup</attribute>
        <attribute name="action">app.about</attribute>
      </item>
    </section>
  </menu>
  <menu id="detail_menu">
    <section>
      <item>
        <attribute name="label" translatable="yes">Delete backup configuration</attribute>
        <attribute name="action">app.remove</attribute>
      </item>
    </section>
  </menu>
  <object class="GtkPopover" id="pending_popover">
    <property name="child">
      <object class="GtkLabel">
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <property name="margin-top">12</property>
        <property name="margin-bottom">12</property>
        <property name="label" translatable="yes">Archives are being mounted.
This might take a while.</property>
      </object>
    </property>
  </object>
  <object class="GtkAdjustment" id="schedule_keep_annual_adj">
    <property name="upper">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="schedule_keep_daily_adj">
    <property name="lower">1</property>
    <property name="upper">99</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="schedule_keep_monthly_adj">
    <property name="upper">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="schedule_keep_weekly_adj">
    <property name="lower">1</property>
    <property name="upper">99</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkPopover" id="schedule_preferred_day_popover">
    <property name="child">
      <object class="GtkCalendar" id="schedule_preferred_day_calendar">
        <property name="year">2021</property>
        <property name="month">1</property>
        <property name="day">1</property>
        <property name="show-heading">0</property>
        <property name="show-day-names">0</property>
      </object>
    </property>
  </object>
  <object class="GtkAdjustment" id="schedule_preferred_hour_adj">
    <property name="upper">23</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="schedule_preferred_minute_adj">
    <property name="upper">59</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkPopover" id="schedule_preferred_time_popover">
    <property name="child">
      <object class="GtkBox">
        <property name="margin-start">18</property>
        <property name="margin-end">18</property>
        <property name="margin-top">18</property>
        <property name="margin-bottom">18</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkSpinButton" id="schedule_preferred_hour">
            <property name="max-width-chars">2</property>
            <property name="orientation">vertical</property>
            <property name="adjustment">schedule_preferred_hour_adj</property>
            <property name="numeric">1</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <property name="label" translatable="yes">:</property>
          </object>
        </child>
        <child>
          <object class="GtkSpinButton" id="schedule_preferred_minute">
            <property name="max-width-chars">2</property>
            <property name="orientation">vertical</property>
            <property name="adjustment">schedule_preferred_minute_adj</property>
            <property name="numeric">1</property>
          </object>
        </child>
      </object>
    </property>
  </object>
  <object class="AdwApplicationWindow" id="window">
    <property name="default-width">782</property>
    <property name="default-height">626</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar">
            <child type="title">
              <object class="AdwViewSwitcherTitle" id="view_switcher_title">
                <property name="stack">detail_stack</property>
                <property name="title" translatable="yes">Pika Backup</property>
              </object>
            </child>
            <child type="end">
              <object class="GtkMenuButton" id="pending_menu">
                <property name="visible">0</property>
                <property name="popover">pending_popover</property>
                <child>
                  <object class="GtkSpinner" id="pending_menu_spinner"/>
                </child>
                <style>
                  <class name="image-button"/>
                </style>
              </object>
            </child>
            <child type="start">
              <object class="GtkButton" id="add_backup">
                <property name="tooltip-text" translatable="yes">Add backup configuration</property>
                <property name="icon-name">list-add-symbolic</property>
              </object>
            </child>
            <child type="end">
              <object class="GtkMenuButton" id="primary_menu_button">
                <property name="menu-model">app_menu</property>
                <property name="icon-name">open-menu-symbolic</property>
              </object>
            </child>
            <child type="end">
              <object class="GtkMenuButton" id="secondary_menu_button">
                <property name="menu-model">detail_menu</property>
                <property name="icon-name">view-more-symbolic</property>
              </object>
            </child>
            <child type="start">
              <object class="GtkButton" id="back_button">
                <property name="tooltip-text" translatable="yes">Back to overview</property>
                <property name="icon-name">go-previous-symbolic</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="hexpand">1</property>
            <property name="vexpand">1</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkStack" id="main_stack">
                <property name="vexpand">1</property>
                <property name="hhomogeneous">0</property>
                <property name="vhomogeneous">0</property>
                <property name="transition-type">crossfade</property>
                <child>
                  <object class="GtkBox" id="page_detail">
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkInfoBar" id="internal_message">
                        <property name="message-type">warning</property>
                        <property name="revealed">0</property>
                        <property name="show-close-button">1</property>
                        <child>
                          <object class="GtkLabel" id="internal_message_text"/>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="AdwViewStack" id="detail_stack">
                        <property name="vexpand">1</property>
                        <child>
                          <object class="AdwViewStackPage">
                            <property name="title" translatable="yes">Backup</property>
                            <property name="icon-name">media-playback-start-symbolic</property>
                            <property name="child">
                              <object class="GtkScrolledWindow" id="page_backup">
                                <property name="child">
                                  <object class="GtkViewport" id="viewport_detail">
                                    <child>
                                      <object class="AdwClamp">
                                        <property name="margin-start">16</property>
                                        <property name="margin-end">16</property>
                                        <property name="margin-start">16</property>
                                        <property name="margin-end">16</property>
                                        <property name="margin-top">30</property>
                                        <property name="margin-bottom">16</property>
                                        <property name="maximum-size">550</property>
                                        <property name="tightening-threshold">450</property>
                                        <child>
                                          <object class="GtkBox">
                                            <property name="orientation">vertical</property>
                                            <property name="spacing">16</property>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="orientation">vertical</property>
                                                <property name="spacing">18</property>
                                                <child>
                                                  <object class="GtkListBox" id="target_listbox">
                                                    <property name="selection-mode">none</property>
                                                    <child>
                                                      <object class="AdwActionRow" id="detail_status_row">
                                                        <property name="activatable">False</property>
                                                        <property name="selectable">False</property>
                                                        <child>
                                                          <object class="GtkBox" id="detail_status_right">
                                                            <child>
                                                              <object class="GtkButton" id="stop_backup_create">
                                                                <property name="visible">0</property>
                                                                <property name="label" translatable="yes">_Abort</property>
                                                                <property name="halign">center</property>
                                                                <property name="valign">center</property>
                                                                <property name="use-underline">1</property>
                                                              </object>
                                                            </child>
                                                            <child>
                                                              <object class="GtkImage" id="detail_hint_icon">
                                                                <property name="visible">0</property>
                                                                <property name="icon-name">go-next-symbolic</property>
                                                                <style>
                                                                  <class name="dim-label"/>
                                                                </style>
                                                              </object>
                                                            </child>
                                                          </object>
                                                        </child>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="AdwActionRow" id="detail_repo_row"/>
                                                    </child>
                                                    <style>
                                                      <class name="content"/>
                                                    </style>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkButton" id="backup_run">
                                                    <property name="label" translatable="yes">_Back Up Now</property>
                                                    <property name="halign">center</property>
                                                    <property name="valign">start</property>
                                                    <property name="use-underline">1</property>
                                                    <style>
                                                      <class name="suggested-action"/>
                                                      <class name="text-button"/>
                                                      <class name="pill"/>
                                                    </style>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="orientation">vertical</property>
                                                <property name="spacing">6</property>
                                                <child>
                                                  <object class="GtkBox">
                                                    <child>
                                                      <object class="GtkLabel">
                                                        <property name="label" translatable="yes">Files to backup</property>
                                                        <property name="halign">start</property>
                                                        <property name="hexpand">1</property>
                                                        <property name="hexpand-set">1</property>
                                                        <style>
                                                          <class name="heading"/>
                                                        </style>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkButton" id="add_include">
                                                        <property name="tooltip-text" translatable="yes">Include directory</property>
                                                        <property name="icon-name">list-add-symbolic</property>
                                                        <style>
                                                          <class name="flat"/>
                                                        </style>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkListBox" id="include">
                                                    <property name="selection-mode">none</property>
                                                    <child>
                                                      <object class="AdwActionRow" id="include_home_row">
                                                        <property name="name">-home</property>
                                                        <property name="title" translatable="yes">Home</property>
                                                        <property name="subtitle" translatable="yes">Usually contains all personal data</property>
                                                        <child type="prefix">
                                                          <object class="GtkImage">
                                                            <property name="icon-name">user-home</property>
                                                            <style>
                                                              <class name="row-icon"/>
                                                            </style>
                                                          </object>
                                                        </child>
                                                        <child>
                                                          <object class="GtkSwitch" id="include_home">
                                                            <property name="halign">end</property>
                                                            <property name="valign">center</property>
                                                            <property name="active">1</property>
                                                          </object>
                                                        </child>
                                                      </object>
                                                    </child>
                                                    <style>
                                                      <class name="content"/>
                                                    </style>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="orientation">vertical</property>
                                                <property name="spacing">6</property>
                                                <child>
                                                  <object class="GtkBox">
                                                    <child>
                                                      <object class="GtkLabel">
                                                        <property name="label" translatable="yes">Exclude from backup</property>
                                                        <property name="halign">start</property>
                                                        <property name="hexpand">1</property>
                                                        <property name="hexpand-set">1</property>
                                                        <style>
                                                          <class name="heading"/>
                                                        </style>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkButton" id="add_exclude">
                                                        <property name="tooltip-text" translatable="yes">Add excluded directory</property>
                                                        <property name="icon-name">list-add-symbolic</property>
                                                        <style>
                                                          <class name="flat"/>
                                                        </style>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkStack" id="detail_exclude_stack">
                                                    <child>
                                                      <object class="GtkListBox" id="backup_exclude">
                                                        <property name="selection-mode">none</property>
                                                        <style>
                                                          <class name="content"/>
                                                        </style>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkStackPage">
                                                        <property name="child">
                                                          <object class="GtkLabel" id="detail_exclude_placeholder">
                                                            <property name="label" translatable="yes">Nothing excluded from backup</property>
                                                            <style>
                                                              <class name="dim-label"/>
                                                            </style>
                                                          </object>
                                                        </property>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwViewStackPage">
                            <property name="title" translatable="yes">Archives</property>
                            <property name="icon-name">drive-harddisk-symbolic</property>
                            <property name="child">
                              <object class="GtkScrolledWindow" id="page_archives">
                                <property name="child">
                                  <object class="GtkViewport" id="viewport_archives">
                                    <child>
                                      <object class="AdwClamp">
                                        <property name="margin-start">16</property>
                                        <property name="margin-end">16</property>
                                        <property name="margin-start">16</property>
                                        <property name="margin-end">16</property>
                                        <property name="margin-top">30</property>
                                        <property name="margin-bottom">16</property>
                                        <property name="maximum-size">550</property>
                                        <property name="tightening-threshold">450</property>
                                        <child>
                                          <object class="GtkBox">
                                            <property name="orientation">vertical</property>
                                            <property name="spacing">6</property>
                                            <child>
                                              <object class="GtkBox">
                                                <property name="spacing">6</property>
                                                <child>
                                                  <object class="GtkLabel">
                                                    <property name="halign">start</property>
                                                    <property name="hexpand">1</property>
                                                    <property name="hexpand-set">1</property>
                                                    <property name="label" translatable="yes">Archives</property>
                                                    <style>
                                                      <class name="heading"/>
                                                    </style>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkStack" id="archives_reloading_stack">
                                                    <property name="transition-duration">0</property>
                                                    <child>
                                                      <object class="GtkButton" id="refresh_archives">
                                                        <property name="tooltip-text" translatable="yes">Refresh list of archives</property>
                                                        <property name="icon-name">view-refresh-symbolic</property>
                                                        <style>
                                                          <class name="flat"/>
                                                        </style>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkStackPage">
                                                        <property name="child">
                                                          <object class="GtkSpinner" id="archives_reloading_spinner">
                                                            <property name="halign">center</property>
                                                            <property name="valign">center</property>
                                                          </object>
                                                        </property>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkButton" id="archives_eject_button">
                                                    <property name="tooltip-text" translatable="yes">Stop browsing archive files</property>
                                                    <property name="icon-name">media-eject-symbolic</property>
                                                    <style>
                                                      <class name="flat"/>
                                                    </style>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkStack" id="archives_stack">
                                                <property name="hhomogeneous">0</property>
                                                <property name="vhomogeneous">0</property>
                                                <child>
                                                  <object class="GtkListBox" id="archive_list">
                                                    <property name="selection-mode">none</property>
                                                    <style>
                                                      <class name="content"/>
                                                    </style>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkStackPage">
                                                    <property name="child">
                                                      <object class="GtkLabel" id="archive_list_placeholder">
                                                        <property name="label" translatable="yes">No archives available</property>
                                                        <style>
                                                          <class name="dim-label"/>
                                                        </style>
                                                      </object>
                                                    </property>
                                                  </object>
                                                </child>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </property>
                              </object>
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwViewStackPage">
                            <property name="title" translatable="yes">Schedule</property>
                            <property name="icon-name">alarm-symbolic</property>
                            <property name="child">
                              <object class="GtkScrolledWindow" id="page_schedule">
                                <property name="child">
                                  <object class="GtkViewport">
                                    <child>
                                      <object class="AdwClamp">
                                        <property name="margin-start">16</property>
                                        <property name="margin-end">16</property>
                                        <property name="margin-top">30</property>
                                        <property name="margin-bottom">18</property>
                                        <property name="maximum-size">550</property>
                                        <property name="tightening-threshold">450</property>
                                        <child>
                                          <object class="GtkBox">
                                            <property name="orientation">vertical</property>
                                            <property name="spacing">30</property>
                                            <child>
                                              <object class="GtkListBox">
                                                <property name="visible">0</property>
                                                <property name="selection-mode">none</property>
                                                <child>
                                                  <object class="AdwActionRow">
                                                    <property name="title" translatable="yes">Next Scheduled Backup: Today, 14:26</property>
                                                    <property name="icon-name">appointment-new-symbolic</property>
                                                    <property name="subtitle" translatable="yes">Will not start until connected to battery</property>
                                                    <child>
                                                      <object class="GtkToggleButton" id="schedule_pause">
                                                        <property name="valign">center</property>
                                                        <child>
                                                          <object class="GtkImage">
                                                            <property name="margin-start">5</property>
                                                            <property name="margin-end">5</property>
                                                            <property name="margin-top">5</property>
                                                            <property name="margin-bottom">5</property>
                                                            <property name="icon-name">media-playback-pause-symbolic</property>
                                                          </object>
                                                        </child>
                                                        <style>
                                                          <class name="circular"/>
                                                        </style>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                                <style>
                                                  <class name="content"/>
                                                </style>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkListBox">
                                                <property name="selection-mode">none</property>
                                                <child>
                                                  <object class="AdwExpanderRow" id="schedule_active">
                                                    <property name="activatable">False</property>
                                                    <property name="title">Periodic Backups</property>
                                                    <property name="subtitle">BETA</property>
                                                    <property name="expanded">True</property>
                                                    <property name="show-enable-switch">True</property>
                                                    <child>
                                                      <object class="AdwComboRow" id="schedule_frequency">
                                                        <property name="title" translatable="yes">Frequency</property>
                                                        <property name="expression">
                                                          <lookup type="PikaBackupScheduleFrequency" name="display"/>
                                                        </property>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="AdwActionRow" id="preferred_time_row">
                                                        <property name="title" translatable="yes">Preferred Time</property>
                                                        <property name="activatable-widget">schedule_preferred_time_button</property>
                                                        <child>
                                                          <object class="GtkMenuButton" id="schedule_preferred_time_button">
                                                            <property name="focus-on-click">0</property>
                                                            <property name="halign">end</property>
                                                            <property name="valign">center</property>
                                                            <property name="popover">schedule_preferred_time_popover</property>
                                                            <property name="label" translatable="yes">14:26</property>
                                                            <style>
                                                              <class name="flat"/>
                                                            </style>
                                                          </object>
                                                        </child>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="AdwComboRow" id="preferred_weekday_row">
                                                        <property name="expression">
                                                          <lookup type="PikaBackupWeekday" name="display"/>
                                                        </property>
                                                        <property name="title" translatable="yes">Preferred Weekday</property>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="AdwActionRow" id="preferred_day_row">
                                                        <property name="title" translatable="yes">Preferred Day</property>
                                                        <property name="activatable-widget">schedule_preferred_day</property>
                                                        <child>
                                                          <object class="GtkMenuButton" id="schedule_preferred_day">
                                                            <property name="focus-on-click">0</property>
                                                            <property name="halign">end</property>
                                                            <property name="valign">center</property>
                                                            <property name="popover">schedule_preferred_day_popover</property>
                                                            <property name="label" translatable="yes">21st</property>
                                                            <style>
                                                              <class name="flat"/>
                                                            </style>
                                                          </object>
                                                        </child>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                                <style>
                                                  <class name="content"/>
                                                </style>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkListBox">
                                                <property name="visible">0</property>
                                                <property name="selection-mode">none</property>
                                                <child>
                                                  <object class="AdwExpanderRow">
                                                    <property name="activatable">False</property>
                                                    <property name="title">Delete Obsolete Backups</property>
                                                    <property name="expanded">True</property>
                                                    <property name="show-enable-switch">True</property>
                                                    <child>
                                                      <object class="AdwActionRow">
                                                        <property name="title" translatable="yes">Keep Daily</property>
                                                        <property name="activatable-widget">schedule_keep_daily</property>
                                                        <child>
                                                          <object class="GtkSpinButton" id="schedule_keep_daily">
                                                            <property name="halign">end</property>
                                                            <property name="valign">center</property>
                                                            <property name="adjustment">schedule_keep_daily_adj</property>
                                                            <property name="climb-rate">1</property>
                                                            <property name="numeric">1</property>
                                                            <property name="value">14</property>
                                                          </object>
                                                        </child>
                                                      </object>
                                                    </child>
                                                    <child>
